<Project>
	<PropertyGroup Condition="$(SolutionDir) == ''">
		<SolutionDir>../../</SolutionDir>
	</PropertyGroup>
	<PropertyGroup>
		<SkipGtkInstall>True</SkipGtkInstall>
		<LangVersion>10.0</LangVersion>
		<Authors>Juan Pablo Calle</Authors>
		<Company>Juan Pablo Calle</Company>
		<Copyright>Copyrigth Â© 2022 Juan Pablo Calle</Copyright>
		<Title>$(MSBuildProjectName)</Title>
		<PackageId>$(MSBuildProjectName)</PackageId>
		<RepositoryType>git</RepositoryType>
		<RepositoryBranch>main</RepositoryBranch>
		<PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<NeutralLanguage>es</NeutralLanguage>
		<RootNamespace>$(MSBuildProjectName)</RootNamespace>
    	<NoWin32Manifest>true</NoWin32Manifest>
    	<Nullable>enable</Nullable>
    	<BaseIntermediateOutputPath>$(SolutionDir).build\obj\$(MSBuildProjectName)</BaseIntermediateOutputPath>
   		<BaseOutputPath>$(SolutionDir).build\bin\$(MSBuildProjectName)</BaseOutputPath>
   		<ApplicationIcon>$(MSBuildThisFileDirectory)../../logo/KyLogo.ico</ApplicationIcon>
	</PropertyGroup>
	
	<PropertyGroup Condition="$(Configuration)=='Release'">
		<DebugType>none</DebugType>
	</PropertyGroup>
</Project>
